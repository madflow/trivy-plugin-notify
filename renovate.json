{
  "assignees": ["madflow"],
  "extends": ["config:recommended"],
  "labels": ["dependencies"],
  "timezone": "Europe/Berlin",
  "ignorePaths": ["**/testdata/**"],
  "internalChecksFilter": "strict",
  "minimumReleaseAge": "7 days",
  "osvVulnerabilityAlerts": true,
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ]
}
